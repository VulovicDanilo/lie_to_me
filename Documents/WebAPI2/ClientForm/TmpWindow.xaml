<Window x:Class="ClientForm.TmpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClientForm"
        mc:Ignorable="d"
        Title="TmpWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="lbxInfo" HorizontalAlignment="Left" Height="366" VerticalAlignment="Top" Width="274" Canvas.Left="508" Canvas.Top="43"/>
        <Canvas x:Name="cnvMain" HorizontalAlignment="Left" Height="419" VerticalAlignment="Top" Width="792">
            <Label Content="Info" HorizontalAlignment="Left" VerticalAlignment="Top" Height="28" Canvas.Left="508" Canvas.Top="12" RenderTransformOrigin="0.613,-0.411"/>
            <Button x:Name="btnStart" Content="start" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click="BtnStart_Click" Canvas.Left="10" Canvas.Top="10"/>
            <Label x:Name="lblPlayerCount" Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Left="10" Canvas.Top="383"/>
        </Canvas>
        <Canvas x:Name="cnvSecond" HorizontalAlignment="Left" Height="419" VerticalAlignment="Top" Width="508">
            <Button x:Name="btnBack" Content="back" Width="75" Canvas.Top="10" Click="BtnBack_Click" Canvas.Left="10"/>
        </Canvas>

    </Grid>
</Window>
